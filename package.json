{
  "name": "street-manager-jobs",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "tslint --project tslint.json --exclude 'prod_node_modules/**'",
    "job-1": "node ./dist/job-1/app.js",
    "test": "TZ=UTC npm run lint && nyc npm run test-unit",
    "test-unit": "TZ=UTC mocha 'tests/unit/**/*.ts' --require ts-node/register",
    "test-integration": "mocha 'tests/integration/**/*.ts' --require ts-node/register --exit --timeout 15000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/departmentfortransport/street-manager-jobs.git"
  },
  "license": "MIT",
  "dependencies": {
    "inversify": "^5.0.1",
    "knex": "^0.20.11",
    "knex-postgis": "^0.12.0",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/knex": "^0.14.19",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@types/sinon": "^5.0.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "sinon": "^7.3.2",
    "ts-mockito": "^2.3.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.8.3"
  }
}
